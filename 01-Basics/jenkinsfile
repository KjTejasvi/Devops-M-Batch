pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                sh '''
                    echo "My Name is Allamesh K J"
                '''
            }
        }
        stage('Bye') {
            steps {
                sh '''
                    echo "This is My First Pipeline"
                '''
            }
        }
        stage('Final Stage') {
            steps {
                sh '''
                    echo "Pipeline Executed Successfully"
                '''     
            }
        }
    }
}